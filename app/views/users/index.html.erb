<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      List of all users
    </h1>
  </div>
</div>
  <a href="home">Return to Home Page</a>
<hr>

<hr>
<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Email
        </th>

        <th>
          Password
        </th>

        <th>
          Phone number
        </th>

        <th>
          Student
        </th>

        <th>
          Password digest
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td>
          <%= user.id %>
        </td>

        <td>
          <%= user.email %>
        </td>

        <td>
          <%= user.password %>
        </td>

        <td>
          <%= user.phone_number %>
        </td>

        <td>
          <%= user.student_id %>
        </td>

        <td>
          <%= user.password_digest %>
        </td>

        <td>
          <%= time_ago_in_words(user.created_at) %> ago
        </td>
        <td>
          <%= time_ago_in_words(user.updated_at) %> ago
        </td>

        <td>
          <a href="/users/<%= user.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

<hr>
